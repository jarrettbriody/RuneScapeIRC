  "bugs": {
    "url": "https://github.com/jarrettbriody/RuneScapeIRC/issues"
  },
  "homepage": "https://github.com/jarrettbriody/RuneScapeIRC#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "websocket": "^1.0.28"
  }
}
