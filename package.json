{
  "name": "runescapeirc",
  "version": "1.0.0",
  "description": "An IRC for RuneScape players with functionality pertaining to RuneScape",
  "main": "./server/server.js",
  "scripts": {
    "test": "echo \"Tests complete\"",
    "start": "node ./server/server.js",
    "pretest": "eslint ./server --fix",
    "build": "babel ./client --out-file ./hosted/bundle.js",
    "watch": "babel --watch ./client --out-file ./hosted/bundle.js",
    "nodemon": "nodemon --watch ./server ./server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jarrettbriody/RuneScapeIRC.git"
  },
